<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.klistret.com/cmdb"
	xmlns:cmdb="http://www.klistret.com/cmdb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">
	<xs:include schemaLocation="commons.xsd" />

	<xs:complexType name="Element">
		<xs:annotation>
			<xs:documentation>Root CI (abstract) defining common CI
				characteristics</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cmdb:Base" />
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="Element" type="cmdb:Element" abstract="true"></xs:element>
</schema>