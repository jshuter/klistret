<schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.klistret.com/cmdb/ci/element/logical/collection"
	xmlns:collection="http://www.klistret.com/cmdb/ci/element/logical/collection"
	xmlns:logical="http://www.klistret.com/cmdb/ci/element/logical"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:ci="http://www.klistret.com/xjc/plugin/ci"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	jaxb:version="2.0"
	jaxb:extensionBindingPrefixes="ci">
	<xs:import
		namespace="http://www.klistret.com/cmdb/ci/element/logical"
		schemaLocation="../collection.xsd" />

	<xs:complexType name="Environment">
		<xs:annotation>
			<xs:documentation></xs:documentation>
			<xs:appinfo>
				<ci:element />
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="logical:Collection" />
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="Environment" type="collection:Environment"></xs:element>
</schema>