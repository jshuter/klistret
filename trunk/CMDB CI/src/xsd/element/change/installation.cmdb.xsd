<xs:schema xmlns="http://www.klistret.com/cmdb/ci/element/change"
	targetNamespace="http://www.klistret.com/cmdb/ci/element/change"
	xmlns:element="http://www.klistret.com/cmdb/ci/element"
	xmlns:commons="http://www.klistret.com/cmdb/ci/commons"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">

	<xs:import namespace="http://www.klistret.com/cmdb/ci/commons"
		schemaLocation="../../commons.cmdb.xsd" />

	<xs:import namespace="http://www.klistret.com/cmdb/ci/element"
		schemaLocation="../change.cmdb.xsd" />

	<xs:complexType name="Installation">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="element:Change">
				<xs:sequence>
					<xs:element name="System"
						type="commons:SourceReference" maxOccurs="1" minOccurs="1" />
					<xs:element name="Software"
						type="commons:DestinationReference" maxOccurs="1" minOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="Installation" type="Installation"></xs:element>
</xs:schema>