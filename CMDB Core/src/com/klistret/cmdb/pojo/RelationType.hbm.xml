<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.klistret.cmdb.xmlbeans.pojo">

	<class name="RelationType" table="RELATIONTYPE" entity-name="RelationType">

		<id name="id">
			<column name="RELATIONTYPEID" sql-type="INTEGER" />
			<generator class="identity">
			</generator>
		</id>

		<property name="name">
			<column name="NAME" length="100" not-null="true" />
		</property>

		<property name="fromTimeStamp" type="com.klistret.cmdb.utility.hibernate.HibernateUTC$CalendarType">
            <column name="FROMTIMESTAMP" not-null="true" sql-type="timestamp" default="current timestamp"/>
        </property>
        
        <property name="toTimeStamp" type="com.klistret.cmdb.utility.hibernate.HibernateUTC$CalendarType">
            <column name="TOTIMESTAMP" sql-type="timestamp"/>
        </property>
        
        <property name="createId">
            <column name="CREATEID" length="10"/>
        </property>
        
        <property name="createTimeStamp" type="com.klistret.cmdb.utility.hibernate.HibernateUTC$CalendarType">
            <column name="CREATETIMESTAMP" not-null="true" sql-type="timestamp" default="current timestamp"/>
        </property>
        
        <property name="updateTimeStamp" type="com.klistret.cmdb.utility.hibernate.HibernateUTC$CalendarType">
            <column name="UPDATETIMESTAMP" not-null="true" sql-type="timestamp" default="current timestamp"/>
        </property>

	</class>

</hibernate-mapping>